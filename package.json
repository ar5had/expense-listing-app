    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.0",
    "apollo-server-express": "^2.9.2",
    "apollo-upload-client": "^11.0.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "graphql": "^14.5.3",
    "graphql-tag": "^2.10.1",
    "moment": "^2.24.0",
    "next": "^9.0.5",
