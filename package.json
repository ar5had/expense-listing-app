{
  "name": "pleo-frontend-challenge",
  "version": "1.0.0",
  "description": "Implement an expenses list fetching all expenses from the provided API. Allow the user to add notes and upload receipt pictures to each expense.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "remove-dist": "rimraf ./dist",
    "build:client": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:client && npm run remove-dist && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/server/app.js",
    "heroku:start": "node dist/server/app.js",
    "prettier": "prettier --write './**/*.{ts,tsx}' --config ./.prettierrc"
  },
  "keywords": [
    "next.js",
    "react.js",
    "typescript",
    "Arshad Khan",
    "graphql",
    "styled-components"
  ],
  "author": "Arshad Khan<arshdkhn1@gmail.com>",
  "license": "ISC",
